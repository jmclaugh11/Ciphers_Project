    "    \"\\n\",\n",
    "    \"    for key in cipher_bet:\\n\",\n",
    "    \"        letter = str(cipher_bet[key])\\n\",\n",
    "    \"        # check if the keys and values are letters in the alphabet\\n\",\n",
    "    \"        if key.lower() in ALPHABET and letter.lower() in ALPHABET:\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if letter.lower() in tracked_letters:\\n\",\n",
    "    \"                print(\\\"Repeated letter (value)\\\")\\n\",\n",
    "    \"                return False\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                tracked_letters.append(letter.lower())\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            print(\\\"Key/Value not in alphabet\\\")\\n\",\n",
    "    \"            return False\\n\",\n",
    "    \"\\n\",\n",
    "    \"    #check number of pairs\\n\",\n",
    "    \"    if len(tracked_letters) != len(ALPHABET):\\n\",\n",
    "    \"        print(\\\"Invalid cipher length\\\")\\n\",\n",
    "    \"        return False\\n\",\n",
    "    \"\\n\",\n",
    "    \"    return True\"\n",
